var apxTag = window.apxTag || {};
apxTag.cmd = apxTag.cmd || [];
apxTag.data = apxTag.data || {};
apxTag.init = true;

window.googletag = window.googletag || {};
window.googletag.cmd = window.googletag.cmd || [];

(function(){
    apxTag.data.zones = apxTag.data.zones || {};
    apxTag.data.zones["WhuQmzOp"] = apxTag.data.zones["WhuQmzOp"] || {};
    apxTag.data.zones["WhuQmzOp"].cid = "5_Bhm8JaTiS0f6yDVBWboQ";
    apxTag.data.zones["WhuQmzOp"].crid = "vnGkzgHDQwSd0dIZhPBzig";
    window.googletag.cmd.push(function() {
      var pubads = window.googletag.pubads();
      pubads.setTargeting("apxzone_WhuQmzOp", "1");
      pubads.setTargeting("apx_info", encodeURIComponent(JSON.stringify({"page_title": document.title})));
    });
})();

