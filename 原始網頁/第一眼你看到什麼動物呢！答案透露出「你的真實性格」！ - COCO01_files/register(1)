function onLoginCallback(n){console.log(n);n.responseJSON==undefined?$("#loginForm").html(n.responseText):window.location.reload()}function onRegisterCallback(n){n.responseJSON==undefined?$("#registerForm").html(n.responseText):window.location.reload()}$(function(){$(document).on("click","#loginBtn",function(){var n=$("#loginForm form");n.valid()&&($(this).toggleClass("hidden"),$("#loginLoadingBtn").toggleClass("hidden"),n.submit())});$(document).on("click","#registerBtn",function(){var n=$("#registerForm form");n.valid()&&($(this).toggleClass("hidden"),$("#registerLoadingBtn").toggleClass("hidden"),n.submit())})})